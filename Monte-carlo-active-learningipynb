    "######   (ii) Semi-Supervised Learning"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 112,
   "metadata": {},
   "outputs": [],
   "source": [
    "# Normalized training Data\n",
    "npos_train = Xtrain[Ytrain == 'M']\n",
    "npos_train.reset_index(drop=True,inplace=True)\n",
    "temp = Ytrain[Ytrain == 'M']\n",
    "temp.reset_index(drop=True,inplace=True)\n",
    "npos_train = pd.concat([npos_train,temp],axis = 1)\n",
    "npos_train.columns = range(31)\n",
    "\n",
    "nneg_train = Xtrain[Ytrain == 'B']\n",
    "nneg_train.reset_index(drop=True,inplace=True)\n",
    "temp = Ytrain[Ytrain=='B']\n",
    "temp.reset_index(drop=True,inplace=True)\n",
    "nneg_train = pd.concat([nneg_train,temp],axis = 1)\n",
    "nneg_train.columns = range(31)\n"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 111,
   "metadata": {},
   "outputs": [
    {
